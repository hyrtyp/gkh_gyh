<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/welcome_background" >

    <RelativeLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        >
        <ImageView 
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/guokaihang_logo"
            android:layout_centerVertical="true"
            android:layout_marginLeft="50dip"
            />
        <ImageView
        	android:id="@id/homepage_dzb_top"
       		android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:src="@drawable/guokaihang_top_home" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@id/botton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" 
        android:visibility="gone">

        <ImageView
            android:id="@id/home_ceinet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="@drawable/homepage_botton_button_3" />

        <ImageView
            android:id="@id/home_witsea"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@id/home_ceinet"
            android:src="@drawable/homepage_botton_button_2" />

        <ImageView
            android:id="@id/home_main_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@id/home_witsea"
            android:src="@drawable/homepage_botton_num" />

        <ImageView
            android:id="@id/home_announcement"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@id/home_main_num"
            android:src="@drawable/homepage_botton_button_1" />

        <ImageView
            android:id="@id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@id/home_announcement"
            android:src="@drawable/homepage_botton_left" />

        <ImageView
            android:id="@id/home_personcenter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@id/home_ceinet"
            android:src="@drawable/homepage_botton_button_4" />

        <ImageView
            android:id="@id/home_disclaimer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@id/home_personcenter"
            android:src="@drawable/homepage_botton_button_5" />

        <ImageView
            android:id="@id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@id/home_disclaimer"
            android:src="@drawable/homepage_botton_right" />
    </RelativeLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="#e0ffffff">

        <RelativeLayout
            android:id="@id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/guokaihang_login_03"
            android:visibility="gone">

            <RelativeLayout
                android:layout_width="100.0dip"
                android:layout_height="100.0dip"
                android:layout_centerInParent="true"
                android:visibility="gone">

                <Button
                    android:id="@id/back_back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/back_back" />
            </RelativeLayout>
        </RelativeLayout>

        <ImageView
            android:id="@id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@id/imageView3"
            android:background="@drawable/login_9"
            android:visibility="gone"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="用户注册"
            android:textColor="#ff000000"
            android:textSize="40sp"
            android:gravity="center"
            android:layout_marginTop="20dp"
            />
        <ScrollView
            android:id="@id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@id/ui_regist_etparent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="30dp"
                >

                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview111"
                        android:layout_width="120.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="用户名 :"
                        android:textColor="#ff000000"
                        android:textSize="25.0sp" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview111"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150dp"
                        android:paddingLeft="10.0dip"
                        android:singleLine="true"
                        android:layout_marginRight="30dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10.0dip"
                        android:background="@drawable/grzx_star" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview222"
                        android:layout_width="120.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="密码 :"
                        android:textColor="#ff000000"
                        android:textSize="25.0sp" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview222"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150.0dip"
                        android:paddingLeft="10.0dip"
                        android:password="true"
                        android:singleLine="true"
                        android:hint="六位数字"
                        android:layout_marginRight="30dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10.0dip"
                        android:background="@drawable/grzx_star" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview333"
                        android:layout_width="120.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="密码确认 :"
                        android:textColor="#ff000000"
                        android:textSize="25.0sp" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview333"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150.0dip"
                        android:paddingLeft="10.0dip"
                        android:password="true"
                        android:singleLine="true"
                        android:hint="六位数字"
                        android:layout_marginRight="30dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20.0dip"
                        android:background="@drawable/grzx_star" />
                </RelativeLayout>



                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview555"
                        android:layout_width="120.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="电子邮箱 :"
                        android:textColor="#ff000000"
                        android:textSize="25.0sp" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview555"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150.0dip"
                        android:paddingLeft="10.0dip"
                        android:singleLine="true"
                        android:hint="用于找回密码"
                        android:layout_marginRight="30dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20.0dip"
                        android:background="@drawable/grzx_star" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview444"
                        android:layout_width="120.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="单位名称 :"
                        android:textColor="#ff000000"
                        android:textSize="25.0sp" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview444"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150.0dip"
                        android:paddingLeft="10.0dip"
                        android:singleLine="true"
                        android:layout_marginRight="30dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:visibility="invisible"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20.0dip"
                        android:background="@drawable/grzx_star" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview666"
                        android:layout_width="100.0dip"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="right"
                        android:text="性别 :"
                        android:textColor="#ff000000"
                        android:textSize="20.0sp" />

                    <RadioGroup
                        android:id="@id/lines"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview666"
                        android:checkedButton="@id/wlan"
                        android:orientation="horizontal" >

                        <RadioButton
                            android:id="@id/economic_register_register_query_accurate_query_radio"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20.0dip"
                            android:checked="true"
                            android:text="男"
                            android:textColor="#ff000000" />

                        <RadioButton
                            android:id="@id/economic_register_register_qussssery_vague_query_radio"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20.0dip"
                            android:text="女"
                            android:textColor="#ff000000" />
                    </RadioGroup>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:visibility="gone"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview777"
                        android:layout_width="120.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="证件类型 :"
                        android:textColor="#ff000000"
                        android:textSize="20.0sp" />

                    <EditText
                        android:id="@id/regist_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview777"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150.0dip"
                        android:paddingLeft="10.0dip"
                        android:singleLine="true" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview888"
                        android:layout_width="100.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="证件号码 :"

                        android:textColor="#ff000000"
                        android:textSize="20.0sp" />

                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview888"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150.0dip"
                        android:paddingLeft="10.0dip"
                        android:singleLine="true" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20.0dip" >

                    <TextView
                        android:id="@id/textview999"
                        android:layout_width="120.0dip"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="电话 :"
                        android:textColor="#ff000000"
                        android:textSize="25.0sp" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20.0dip"
                        android:layout_toRightOf="@id/textview999"
                        android:background="@drawable/ui_login_etborder"
                        android:maxWidth="150.0dip"
                        android:paddingLeft="10.0dip"
                        android:singleLine="true"
                        android:layout_marginRight="30dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="450.0dip"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30.0dip" >

                    <Button
                        android:id="@id/ui_regist_regist"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/guokaihang_login_btn"
                        android:layout_alignParentRight="true"/>


                    <Button
                        android:id="@+id/ui_regist_login"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/guokaihang_regist_bt"
                        android:layout_alignParentLeft="true"/>
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>

        <ImageView
            android:id="@id/imageView99"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/imageView3"
            android:layout_alignRight="@id/imageView5"
            android:background="@drawable/login_4"
            android:visibility="gone"/>

        <ImageView
            android:id="@id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageView4"
            android:layout_toRightOf="@id/imageView5"
            android:background="@drawable/login_5"
            android:visibility="gone"/>

        <ImageView
            android:id="@id/ui_regist_regist"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageView6"
            android:layout_toRightOf="@id/imageView5"
            android:background="@drawable/login_6"
            android:visibility="gone"/>
    </LinearLayout>

</RelativeLayout>